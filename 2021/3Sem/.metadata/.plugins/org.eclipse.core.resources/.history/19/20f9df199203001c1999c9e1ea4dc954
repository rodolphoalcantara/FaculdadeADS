package com.rodolphoalcantara.models;

public class Estado {
	
	private String nome;
	private int pib;

	
	public Estado(String nome, int pib) {
		this.nome = nome;
		this.pib = pib;
	}
	
	public Estado() {}
	
	
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public int getPib() {
		return pib;
	}
	public void setPib(int pib) {
		this.pib = pib;
	}
}
